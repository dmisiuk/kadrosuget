<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <servlet>
        <servlet-name>KSController</servlet-name>
        <servlet-class>by.minsler.kadrosuget.controller.KSController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KSController</servlet-name>
        <url-pattern>/ks</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>by.minsler.kadrosuget.controller.AdminController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>

</web-app>
